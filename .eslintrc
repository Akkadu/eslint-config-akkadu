{
  "extends": [
    "plugin:vue/recommended",
    "airbnb-base",
    "prettier/vue",
    "plugin:prettier/recommended"
  ],
  "env": {
      "node": true
  },
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2019,
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": ["error"],
      "no-var": "error",
      "prefer-const": "error",
      "prefer-arrow-callback": "error",
      "no-unused-vars": "warn",
      "quotes": ["error", "single"]
  }
}